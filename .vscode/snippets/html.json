{
  "Hero Unit": {
    "prefix": "_h",
    "body": [
      "<section class=\"hero-unit$1\">",
      "\t<div class=\"grid-container\">",
      "\t\t<div class=\"grid-x grid-margin-x align-center$2\">",
      "\t\t\t\t$3",
      "\t\t</div>",
      "\t</div>",
      "</section>"
    ],
    "description": "Hero Unit"
  },
  "Content Block": {
    "prefix": "_cb",
    "body": [
      "<section class=\"content-block$1\">",
      "\t<div class=\"grid-container\">",
      "\t\t<div class=\"grid-x grid-margin-x align-center$2\">",
      "\t\t\t$3",
      "\t\t</div>",
      "\t</div>",
      "</section>"
    ],
    "description": "Content Block"
	},
	"Section Header": {
    "prefix": "_sh",
    "body": [
			"<div class=\"grid-x grid-margin-x$1\">",
			"\t<div class=\"cell small-11 medium-12\">",
			"\t\t{{> section-header-component headline=\"$2\" $3 }}",
			"\t</div>",
			"</div>"
    ],
    "description": "Header"
  },
  "1 Column": {
    "prefix": "_cols",
    "body": ["<div class=\"cell small-11 medium-${1:12}\">", "\t$2", "</div>"],
    "description": "1 column"
  },
  "2 Columns": {
    "prefix": "_cols",
    "body": [
			"<div class=\"cell small-11 medium-${1:6}\">",
			"\t<div class=\"inner\">",
			"\t\t$2",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${3:6}\">",
			"\t<div class=\"inner\">",
			"\t\t$4",
			"\t</div>",
      "</div>"
    ],
    "description": "2 columns"
  },
  "3 Columns": {
    "prefix": "_cols",
    "body": [
			"<div class=\"cell small-11 medium-${1:4}\">",
			"\t<div class=\"inner\">",
			"\t\t$2",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${3:4}\">",
			"\t<div class=\"inner\">",
			"\t\t$4",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${5:4}\">",
			"\t<div class=\"inner\">",
			"\t\t$6",
			"\t</div>",
      "</div>"
    ],
    "description": "3 columns"
  },
  "4 Columns": {
    "prefix": "_cols",
    "body": [
			"<div class=\"cell small-11 medium-${1:3}\">",
			"\t<div class=\"inner\">",
			"\t\t$2",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${3:3}\">",
			"\t<div class=\"inner\">",
			"\t$4",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${5:3}\">",
			"\t<div class=\"inner\">",
			"\t$6",
			"\t</div>",
      "</div>",
			"<div class=\"cell small-11 medium-${7:3}\">",
			"\t<div class=\"inner\">",
			"\t$8",
			"\t</div>",
      "</div>"
    ],
    "description": "4 columns"
  },
  "Header Component": {
    "prefix": "_c",
    "body": [
      "<header class=\"component stack-order-$1\">",
      "\t<h2 class\"headline\">$2</h2>",
      "\t<h3 class\"subheadline\">$3</h3>",
      "</header>"
    ],
    "description": "Header"
	},
	"Handlebars: Header Component": {
    "prefix": ">c",
    "body": [
      "{{> header-component headline=\"$1\" subheadline=\"$2\" classes=\"$3\" }}"
    ],
    "description": "Panini Header"
  },
  "Text Editor Component": {
    "prefix": "_c",
    "body": [
      "<div class=\"component default stack-order-$1\">",
      "\t$2",
      "</div>"
    ],
    "description": "Text Editor"
	},
	"Handlebars: Text Block Component": {
    "prefix": ">c",
    "body": [
      "{{> text-block-component content=\"$1\" classes=\"$2\" }}"
    ],
    "description": "Panini Text Block"
  },
  "Contact Form Component": {
    "prefix": "_c",
    "body": [
      "<div class=\"component form stack-order-$1\">",
      "\t<form>",
      "\t\t<label>${2:First Name}</label>",
      "\t\t<input type=\"text\" placeholder=\"$2\"/>",
      "\t\t<label>${3:Last Name}</label>",
      "\t\t<input type=\"text\" placeholder=\"$3\"/>",
      "\t\t<label>${4:Email Address}</label>",
      "\t\t<input type=\"email\" placeholder=\"$3\"/>",
      "\t\t<label>${5:Message}</label>",
      "\t\t<textarea placeholder=\"$5\"></textarea>",
      "\t\t<input type=\"${6:Submit}\" class=\"button\"/>",
      "\t</form>",
      "</div>"
    ],
    "description": "Form"
  },
  "Buttons Component": {
    "prefix": "_c",
    "body": [
      "<div class=\"component buttons stack-order-$1\">",
      "\t<a class=\"button\" href=\"${2:#}\">$3</a>",
      "</div>"
    ],
    "description": "Buttons"
	},
	"Handlebars: Button Component": {
    "prefix": ">c",
    "body": [
      "{{> button-component label=\"$1\" url=\"$2\" classes=\"$3\" }}"
    ],
    "description": "Panini Button"
  },
  "Image Component": {
    "prefix": "_c",
    "body": [
      "<div class=\"component image stack-order-$1\">",
      "\t<img src src=\"$2\" alt=\"${3}\" />",
      "</div>"
    ],
    "description": "Image"
	},
	"Handlebars: Image Component": {
    "prefix": ">c",
    "body": [
      "{{> image-component src=\"$1\" classes=\"$2\" }}"
    ],
    "description": "Panini Image"
  },
  "Video Component": {
    "prefix": "_c",
    "body": [
      "<div class=\"component video stack-order-$1\">",
      "\t<div class=\"embed-container widescreen\">",
      "\t\t$2",
      "\t</div>",
      "</div>"
    ],
    "description": "Image"
	},
	"Handlebars: Video Component": {
    "prefix": ">c",
    "body": [
      "{{> video-component src=\"$1\" classes=\"$2\" }}"
    ],
    "description": "Panini Video"
  }
}
